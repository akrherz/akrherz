{
    "git.ignoreLegacyWarning": true,
    "git.confirmSync": false,
    "editor.rulers": [79,],
    "extensions.ignoreRecommendations": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.pytest_cache": true,
        "**/.svn": true,
        "**/*.pyc": true,
        "**/CVS": true
