{
    "git.ignoreLegacyWarning": true,
    "git.confirmSync": false,
    "editor.rulers": [79,],
    "extensions.ignoreRecommendations": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.pytest_cache": true,
        "**/.svn": true,
        "**/*.pyc": true,
        "**/CVS": true
    },
    "files.associations": {
        "*.wsgi": "python",
        "*.tac": "python",
        "*.html": "php"
    },
"workbench.editor.enablePreview": false,
"window.titleBarStyle": "native",
"git.autofetch": true,
"python.pythonPath": "/opt/miniconda3/envs/prod/bin/python",
"python.formatting.provider": "charliermarsh.ruff",
"editor.detectIndentation": false,
"window.zoomLevel": 0,
"python.showStartPage": false,
"jupyter.alwaysTrustNotebooks": true,
"python.languageServer": "Pylance",
"python.analysis.indexing": false,
"python.analysis.nodeExecutable": "/opt/miniconda3/envs/prod/bin/node",
"workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
},
"python.defaultInterpreterPath": "/opt/miniconda3/envs/prod/bin/python",
"notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
},
"editor.inlineSuggest.enabled": true,
"editor.renderWhitespace": "all",
"[yaml]": {
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
},
"github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": false
},
 "markdownlint.config": {
    "MD033": {
